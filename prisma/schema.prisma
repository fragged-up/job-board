// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Job {
  id        String   @id @default(cuid())
  company   String
  position  String
  link      String
  password  String
  createdAt DateTime @default(now())
}
